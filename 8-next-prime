public Integer nextPrime(Integer num) {
    //code here
    if(num<0){
        return 2;
    }
    num++;
   while(num>1){
       if(isPrime(num)){
           break;
       }
       num++;
   }
    return num;   
}
public boolean isPrime(Integer num){
    for(Integer i =2; i<=num/2; i++){
        if(Math.mod(num,i) ==0){
            return false;
        }
    }
    return true;
}