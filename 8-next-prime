public Integer nextPrime(Integer num) {
    //code here
    if(num<0){
        return 2;
    }
    Integer can = num+1;
   while(!isPrime(can)){       
       can++;
   }
    return can;   
}
public boolean isPrime(Integer num){
    Integer sqrt = Math.floor(Math.sqrt(num));
    for(Integer i =2; i<=sqrt; i++){
        if(Math.mod(num,i) ==0){
            return false;
        }
    }
    return true;
}